{
  "$schema": "https://lazykit.jrtilak.dev/schema/config.json",
  "language": "typescript",
  "separate": true,
  "v": "2.0.1",
  "filenameConvention": {
    "helperFunctions": "kebab-case",
    "reactHooks": "kebab-case"
  },
  "paths": {
    "helperFunctions": "@/helpers",
    "reactHooks": "@/hooks"
  },
  "resolve": {
    "alias": {
      "@/helpers": "/src/helpers",
      "@/hooks": "/src/hooks"
    }
  }
}