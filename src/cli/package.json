{
  "name": "@jrtilak/lazykit",
  "version": "1.0.4",
  "description": "Refine your JavaScript workflows with Lazykit. A concentrated collection of lean utility functions, not a bloated library.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://lazykit.thapatilak.com.np/docs/cli",
  "bin": {
    "lazykit": "dist/index.js"
  },
  "repository": {
    "type": "github",
    "url": "git+https://github.com/jrtilak/lazykit.git",
    "directory": "./src/cli"
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --dts --format cjs,esm --out-dir dist",
    "watch": "tsup src/index.ts --minify --dts --format cjs,esm --out-dir dist --watch",
    "lint": "eslint . --ext .ts",
    "dev": "node dist/index.js"
  },
  "keywords": [
    "lazykit",
    "lazy",
    "kit",
    "cli",
    "tool",
    "utility",
    "lazykit-cli",
    "js-utility",
    "utility functions"
  ],
  "author": "Tilak Thapa <iamjrtilak@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-spinners": "^2.9.2",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.16"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8",
    "tsup": "^8.0.2",
    "typescript": "^5"
  }
}
